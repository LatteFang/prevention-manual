.content {
    align-items: center;
    display: flex;
    flex-direction: column;
    justify-content: center;
}

.borderStyle {
    border: 8px solid #3ea6cf;
    border-radius: 10px;
}

.btnView {
    background-color: #1bc78c;
    color: #fff;
    font-size: 32px;
    font-weight: 700;
}

.btnView,.btnView1 {
    border-radius: 10px;
    margin-bottom: 30rpx;
    margin-top: 30rpx;
    padding: 10rpx;
    text-align: center;
}

.btnView1 {
    background-color: #f5f5f5;
    color: #000;
    font-size: 24px;
    font-weight: 500;
}